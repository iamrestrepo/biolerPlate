  },
  "homepage": "https://github.com/iamrestrepo/biolerPlate#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
